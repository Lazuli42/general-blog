<div class="container">
  <div class="jumbotron text-center">
    <h1>bloggsie: home</h1>
  </div>

  {{new-post savePost2="savePost3"}}

  {{#each model as |post|}}
  <div>
    <h2>{{post.title}}</h2>
    <h4>{{post.author}}</h4>
    <img src={{post.image}}>
    <p>
      {{post.content}}
    </p>
    <h6>{{post.time}}</h6>
  </div>
  {{/each}}

</div>
